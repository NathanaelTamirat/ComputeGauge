{
    "version": 2,
    "builds": [
        {
            "src": "api/handler.go",
            "use": "@vercel/go",
            "config": {
                "buildCommand": "chmod +x api/build.sh && ./api/build.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/handler.go"
        }
    ]
}
